{
  // Config Module: Adobe Lightroom Classic
  // Provides verify/restore/capture configuration for Lightroom Classic
  // Curated module - captures presets and preferences, excludes catalog and cache
  
  "id": "apps.lightroom-classic",
  "displayName": "Adobe Lightroom Classic",
  "sensitivity": "low",
  
  "matches": {
    // Lightroom Classic is typically installed via Adobe Creative Cloud (manual install)
    "winget": [],
    "exe": ["lightroom.exe"],
    "uninstallDisplayName": ["^Adobe Lightroom Classic"]
  },
  
  "verify": [
    { "type": "file-exists", "path": "%APPDATA%\\Adobe\\Lightroom\\Preferences\\Lightroom Classic CC 7 Preferences.agprefs" }
  ],
  
  "restore": [
    {
      "type": "copy",
      "source": "./payload/apps/lightroom-classic/Preferences",
      "target": "%APPDATA%\\Adobe\\Lightroom\\Preferences",
      "backup": true,
      "optional": true
    },
    {
      "type": "copy",
      "source": "./payload/apps/lightroom-classic/Develop Presets",
      "target": "%APPDATA%\\Adobe\\Lightroom\\Develop Presets",
      "backup": true,
      "optional": true
    },
    {
      "type": "copy",
